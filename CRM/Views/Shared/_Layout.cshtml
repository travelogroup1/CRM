    <script src="~/Admin Template/plugins/datatables-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Admin Template/plugins/datatables-buttons/js/buttons.print.min.js"></script>
    <script src="~/Admin Template/plugins/datatables-buttons/js/buttons.colVis.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/Admin Template/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Admin Template/dist/js/adminlte.js"></script>
    <!-- PAGE PLUGINS -->
    <!-- jQuery Mapael -->
    <script src="~/Admin Template/plugins/jquery-mousewheel/jquery.mousewheel.js"></script>
    <script src="~/Admin Template/plugins/raphael/raphael.min.js"></script>
    <script src="~/Admin Template/plugins/jquery-mapael/jquery.mapael.min.js"></script>
    <script src="~/Admin Template/plugins/jquery-mapael/maps/usa_states.min.js"></script>
