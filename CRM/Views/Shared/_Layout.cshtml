        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a asp-area="" asp-controller="Home" asp-action="Index" class="brand-link">
                <img src="/Admin Template/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">Card Management Sys</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="/Admin Template/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
                        @*<img src="@Url.Action("Photo", "Home")" class="img-circle elevation-2" alt="User Image" />*@
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">@User.Identity.Name</a>
                    </div>
                </div>
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item ">
                            <a class="nav-link active" asp-area="" asp-controller="Home" asp-action="Index">
                                <i class="nav-icon fas fa-tachometer-alt"></i>
                                <p>
                                    Dashboard
                                </p>
                            </a>
                        </li>
                        <li class="nav-header">Manage Accounts</li>
                        <li class="nav-item">
                            <a asp-area="Identity" asp-page="/Account/Register" class="nav-link">
                                <i class="nav-icon fas fa-user"></i>
                                <p>Add New User</p>
                            </a>
                        </li>
                        <li class="nav-header">Manage Role & Claims</li>
                        <li class="nav-item">
                            <a asp-area="" asp-controller="UserRoles" asp-action="Index" class="nav-link">
                                <i class="nav-icon fas fa-user-check"></i>
                                <p>Add User Role</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Account" asp-action="RoleList">
                                <i class="nav-icon fas fa-user-alt"></i>
                                <p>
                                    Add Claims & Roles
                                </p>
                            </a>
                        </li>
